		TestDataClientUuid:           fenixGuiTestCaseBuilderServerObject.fenixGuiTestCaseBuilderServer_TestDataClientUuid,
		TestDomainUuid:               fenixGuiTestCaseBuilderServerObject.fenixGuiTestCaseBuilderServer_DomainUuid,
		TestDomainName:               fenixGuiTestCaseBuilderServerObject.fenixGuiTestCaseBuilderServer_DomainName,
