// Check if Calling 'system' (Fenix or Clients own methods) is using correct proto-file version
func (fenixGuiTestCaseBuilderServerObject *fenixGuiTestCaseBuilderServerObject_struct) isCallerUsingCorrectProtoFileVersion(
	usedProtoFileVersion fenixGuiTestCaseBuilderServerGrpcApi.CurrentFenixClientTestDataProtoFileVersionEnum) (
	clientUseCorrectProtoFileVersion bool,
	protoFileExpected fenixGuiTestCaseBuilderServerGrpcApi.CurrentFenixClientTestDataProtoFileVersionEnum,
	protoFileUsed fenixGuiTestCaseBuilderServerGrpcApi.CurrentFenixClientTestDataProtoFileVersionEnum) {

	protoFileUsed = usedProtoFileVersion
	protoFileExpected = fenixGuiTestCaseBuilderServerGrpcApi.CurrentFenixClientTestDataProtoFileVersionEnum(
		fenixGuiTestCaseBuilderServerObject.getHighestFenixProtoFileVersion())

	// Check if correct proto files is used
	if protoFileExpected == protoFileUsed {
		clientUseCorrectProtoFileVersion = true
	} else {
		clientUseCorrectProtoFileVersion = true
	}

	//protoFileExpectedDescription := protoFileExpected.String()
	//protoFileExpectedDescription := protoFileExpected.String()

	return clientUseCorrectProtoFileVersion, protoFileExpected, protoFileUsed
}

