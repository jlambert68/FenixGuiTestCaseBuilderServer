	}

	// Set up connection to Server
	fenixGuiTestCaseBuilderServerObject.SetConnectionToFenixTestDataSyncServer()

	// Do gRPC-call
	//ctx := context.Background()
	ctx, cancel := context.WithTimeout(context.Background(), 30*time.Second)
	defer func() {
		fenixGuiTestCaseBuilderServerObject.logger.WithFields(logrus.Fields{
			"ID": "813bf6cb-40ab-4431-aaf4-109c13474482",
		}).Error("Running Defer Cancel function")
		cancel()
	}()

	// Only add access token when run on GCP
	if common_config.ExecutionLocationForFenixTestDataServer == common_config.GCP {

		// Add Access token
		ctx, returnMessageAckNack, _ = fenixGuiTestCaseBuilderServerObject.generateGCPAccessToken(ctx)
		if returnMessageAckNack == false {
			return
		}

	}
	returnMessage, err := fenixGuiTestCaseBuilderServerClient.RegisterTestDataClient(ctx, &TestDataClientInformationMessage)

	// Shouldn't happen
	if err != nil {
		fenixGuiTestCaseBuilderServerObject.logger.WithFields(logrus.Fields{
			"ID":    "6b080a23-4e06-4d16-8295-a67ba7115a56",
			"error": err,
		}).Error("Problem to do gRPC-call to FenixTestDataSyncServer for 'RegisterTestDataClient'")

	} else if returnMessage.AckNack == false {
		// FenixTestDataSyncServer couldn't handle gPRC call
		fenixGuiTestCaseBuilderServerObject.logger.WithFields(logrus.Fields{
			"ID": "44671efb-e24d-450a-acba-006cc248d058",
			"Message from FenixTestDataSyncServerObject": returnMessage.Comments,
		}).Error("Problem to do gRPC-call to FenixTestDataSyncServer for 'RegisterTestDataClient'")
